<div id='id-page-info' class='dappsContainer'>
  <div id='id-container-main'>
    <img class="mainimg" src=<%= @dapp.logo_url %> alt="mainimg">
    <div class="title"><%= @dapp.name %></div>
    <a class="link" href=<%= @dapp.marketing_url %>><%= @dapp.marketing_url %></a>
    <div id='id-div-stars'>
      <% @star.times do |i| %>
        <%= image_tag asset_pack_path('src/imgs/icon/star.svg'), class: 'dappsListImg', alt: 'Logo' %>
      <% end %>

      <% @stargray.times do |i| %>
        <%= image_tag asset_pack_path('src/imgs/icon/stargray.svg'), class: 'dappsListImg', alt: 'Logo' %>
      <% end %>
    </div>
  </div>

  <div id='id-container-intro'>
    <div class="title">藏品描述</div>
    <div class="intro">
      <%= @dapp.desc%>
    </div>
    <div class="time"><%= @start_at%></div>
    <div class="author"><%= @dapp.developer%></div>
  </div>

  <div id="id-container-other">
    <a class="goto" href=<%= @dapp.marketing_url %>>立即前往</a>
    <div class="warning">您即将进入第三方Dapp平台，该服务不由Neruon提供</div>
  </div>
</div>
