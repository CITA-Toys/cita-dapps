<% @dapps.keys.each do |key| %>
  <div class="popularContainer">
    <h3 class="popularTitle">
      <%= key.to_s %>
    </h3>
    <% @dapps[key].each do |dapp| %>

      <div class="dappsListItem">
        <div class="dappsListImgContainer">
          <%= image_tag dapp.logo_url, class: 'dappsListImg', alt: "Logo" %>
        </div>
        <div class="dappsListContent">
          <div class="dappsListName"><%= dapp.name %></div>
          <%= link_to dapp.url_address, dapp.url_address, class: 'dappsListUrl' %>
        </div>
      </div>

    <% end %>

  </div>
<% end %>


